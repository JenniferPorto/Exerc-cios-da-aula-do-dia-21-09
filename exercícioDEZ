  <STyle>
     h1 {
      color:fuchsia;
      background-color: turquoise;
     }
  </STyle>
<script language="javascript">
        var n1,n2, Maior;
        
        n1 = parseInt(window.prompt("Digite um número"));
        n2 = parseInt(window.prompt("Digite outro número"));

        if  (n2>n1)
       {
         maior=n2
        document.write("<h1>O maior número é ",n2,"<h1>" );
       }
      else if  (n1>n2)
      {
        n1=maior
        document.write("<h1>O maior número é ",n1,"<h1>");

      }
      else
      {
        document.write("<h1>Os números são iguais<h1>");
      }
      

       

    </script>
  
</body>
