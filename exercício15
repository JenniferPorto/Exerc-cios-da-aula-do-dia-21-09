<script language="javascript">
        var n1,n2, Maior;
        
        n1 = parseInt(window.prompt("Digite um número:"));
        n2 = parseInt( window.prompt("Digite outro número:"));

        if  (n2>n1)
       {
         maior=n2
        document.write("<h1>O maior número é :",n2,"<h1>" );
       }
      else 
        {
            n2 = parseInt( window.prompt(" O segundo número precisa ser maior que o primeiro!!!!!"));
          
        } 
       
					
      
