<body>
   <STyle>
      h2{
           color:rgb(80, 136, 78);
           background-color:wheat;
       }
      </STyle>
    
<script language="javascript">
        var valor; 
        
       valor=parseInt(window.prompt("Digite um número positivo:"));

       for(i=1; i<=10; i++) 
       
        {
           
         document.write("<h2>",valor, "X", i, "=",i*valor,"<h2>");
        
        }
           
            
        
       
  
    
  

    </script>
  
</body>
